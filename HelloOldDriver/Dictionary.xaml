<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HelloOldDriver"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="AppName">Hello Old Driver</system:String>
    <system:String x:Key="UseHTTPS">Use HTTPS</system:String>
    <system:String x:Key="ContentOnly">Search content only</system:String>
    <system:String x:Key="Domain">Domain</system:String>
    <system:String x:Key="Start">Start</system:String>
    <system:String x:Key="Stop">Stop</system:String>
    <system:String x:Key="Prev">&lt;Prev</system:String>
    <system:String x:Key="Next">Next&gt;</system:String>

    <system:String x:Key="DoubleClickToCopy">Double click an item to copy it to the clipboard.</system:String>
    <system:String x:Key="CopiedToClipboard">The link(s) below have been copied to the clipboard:</system:String>
</ResourceDictionary>